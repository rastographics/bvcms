@model CmsData.Registration.OrgFees.OrgFee
@using CmsWeb
    @using (var ci = Html.BeginCollectionItem(Model.Name))
    {
      <li>@Html.Raw(ci.hiddenindex)
          <ul style="list-style: none;">
              <li>
                  <div class="sgfee">OrgId:</div> @Html.TextBoxFor(m => m.OrgId, new { @class = "short" })
                  <a class="del" href="#"><img align="top" src="/Content/images/delete.gif" title="remove this item" /></a>
              </li>
              <li><div class="sgfee">Fee:</div> @Html.TextBoxFor(m => m.Fee) </li>
          </ul>
      </li>
    }
